            	X_train, X_test = X[:train_pct_index], X[train_pct_index:]
            	y_train, y_test = y[:train_pct_index], y[train_pct_index:]
            	model = MLPClassifier(hidden_layer_sizes = (3, 3), max_iter=400)
            	model.fit(X_train, y_train)
            	return model
            	
